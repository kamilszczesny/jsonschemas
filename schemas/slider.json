{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"itemsNumber": {
            "type": "integer",
            "minimum": 1,
            "maximum": 8
		},
		"displayDurtaion": {
            "type": "integer",
            "minimum": 5,
            "maximum": 30
		},
		"type": {
			"type": "string",
			"enum": [
				"unscheduledList"
			]
		},
		"items": {
			"type": "array",
			"items": [{
				"type": "object",
				"properties": {
					"type": {
						"type": "string",
						"enum": [
							"scheduledList"
						]
					},
					"items": {
						"type": "array",
						"items": [{
							"type": "object",
							"properties": {
								"itemId": {
									"type": "string"
								},
								"itemType": {
									"type": "string",
									"enum": [
										"BannerItem"
									]
								},
								"scheduling": {
									"type": "string"
								}
							},
							"required": [
								"itemId",
								"scheduling"
							]
						}]
					}
				},
				"required": [
					"items"
				]
			}]
		}
	},
	"required": [
		"name",
		"itemsNumber",
		"displayDurtaion",
		"items"
	]
}